generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

type UsersCommittedLog {
  dateCommitted DateTime @db.Date
  timezone      String?
}

model Resource {
  id          String   @id @default(auto()) @map("_id") @db.ObjectId
  count       Int
  description String
  image       String
  tags        String[]
  title       String
  url         String

  @@map("japanese-resources-collection")
}

model User {
  id            String              @id @map("_id")
  avatar        String
  committed     Boolean?
  committedLog  UsersCommittedLog[]
  contact       String
  dateCommitted DateTime?           @db.Date
  name          String
  resources     String[]
  stripeHasCard Boolean?
  stripeId      String?
  timezone      String?
  token         String
}
