  "main": "server/index.ts",
    "scripts": {
    "dev": "nodemon",
    "build": "next build && tsc --project tsconfig.server.json",
    "start": "cross-env NODE_ENV=production ts-node --project tsconfig.server.json -r tsconfig-paths/register dist/server",
    "test": "jest",
    "test:unit": "jest --config tests/jest.unit.config.js",
    "test:integration": "jest --transformIgnorePatterns \"node_modules/(?!@toolz/allow-react)/\" --config tests/jest.integration.config.js",
    "test:watch": "jest --watch --no-cache",
    "lint": "eslint . --ext .js --ext .jsx --ext .ts --ext .tsx",
    "lint:fix": "eslint . --fix --ignore-path .gitignore",
    "codegen:schema": "npx apollo client:download-schema --endpoint=http://localhost:3000/api/graphql",
    "codegen:generate": "npx apollo client:codegen --localSchemaFile=schema.json --includes=graphql/**/*.ts --target=typescript"
  },
  "prisma": {
    "seed": "ts-node --skip-project prisma/seed.ts"
  },
  "engines": {
    "node": "^14.16.1",
    "npm": "^6.14.12"
